{
    "name": "phacts/laravelsentinel",
    "description": "An implementation of the Sentry User Manager for Laravel.",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*@dev",
        "illuminate/contracts": "5.1.*@dev",
        "phacts/sentry": "dev-master",
        "vinkla/hashids": "~2.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.x@dev",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "repositories": [
        {
            "url": "https://github.com/phacts/sentry",
            "type": "vcs"
        }
     ],
    "autoload": {
        "classmap": [
            "src/seeds",
            "src/controllers",
            "tests"
        ],
        "psr-4": {
            "Sentinel\\": "src/Sentinel"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "minimum-stability": "stable"
}
